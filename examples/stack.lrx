int push_stack(tree <int>t(1), int val) {
	tree <int> t2 (1);
	t2 = val[]; // TODO: Change to t2 = val; when semantic checking is fixed
	t = t2 + t;
	return 0;
}
int pop_stack(tree<int>t(1)) {
	tree <int> t2 (1);
	t2 = t;
	t = t2--;
	return t2@;
}

int main() {
	tree <int>t(2);
	push_stack(t, 3);
	push_stack(t, 4);
	push_stack(t, 5);
	print(t);
	pop_stack(t);
	print(t);
}
